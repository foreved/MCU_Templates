    "version": "0.2.0",
    "configurations": [
        {
            "name": "Win Cortex Debug",
            "cwd": "${workspaceFolder}",
            "executable": "build/Debug/task.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "configFiles": [
                "Scripts/Openocd/cmsis-dap.cfg",
                "Scripts/Openocd/stm32h5x.cfg"
            ],
            "svdFile": "Scripts/SVD/STM32H503.svd",
            "gdbPath": "arm-none-eabi-gdb",
            "showDevDebugOutput": "raw"
        }
    ]
}